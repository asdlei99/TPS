	<Control width="182" />
	<VerticalLayout name="SpeakSeat3Layout" width="435" height="140" >
	   <Button  name="btnSpeakSeat3" width="435" height="81" normalimage="res/cnsmanage/speakseatoff.png" />
	</VerticalLayout>
	<Control />
