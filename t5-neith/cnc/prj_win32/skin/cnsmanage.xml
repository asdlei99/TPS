	<HorizontalLayout >
	  <Label name="StcConfName" font="1" text="无会议" textcolor="#FFD4DEEB" align="center" /> 
	</HorizontalLayout>
    <HorizontalLayout name="ConfIconLayout" width="522">
	  <HorizontalLayout name="" padding="0,38,15,53" >
	    <Control />
